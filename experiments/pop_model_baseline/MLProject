name: Popularity Per Month Baseline

entry_points:
  train:
    parameters:
      games_data: { type: path, default: ../../data/steam.duckdb}
    command: "python train.py --games_data {games_data}"
  eval:
    parameters:
      eval_data: { type: path, default: ../../data/steam.duckdb }
    command: "python eval.py --eval_data {eval_data}"
